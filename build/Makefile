ifndef config
  config=debug
endif
export config

PROJECTS := rocky_shores-app

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

rocky_shores-app: 
	@echo "==== Building rocky_shores-app ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f rocky_shores-app.make

clean:
	@${MAKE} --no-print-directory -C . -f rocky_shores-app.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "   debug"
	@echo "   release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   rocky_shores-app"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
